{
  "name": "metro-bootstrap",
  "version": "4.0.0",
  "license": "Apache-2.0",
  "main": "dist/metro-bootstrap.min.css",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "autoprefixer": "^7.2.4",
    "blink-diff": "^1.0.13",
    "bootstrap": "^4.0.0-beta.2",
    "cssnano": "^3.10.0",
    "fs-plus": "^3.0.1",
    "gobble": "^0.12.0",
    "gobble-cli": "^0.8.0",
    "gobble-postcss": "^0.3.0",
    "gobble-sass": "^0.6.0",
    "puppeteer": "^0.13.0",
    "qunit": "^2.4.1",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "serve": "gobble --env=dev serve",
    "generate": "gobble --env=generate --force build .tmp",
    "test": "gobble --env=test --force build .tmp && qunit .tmp/index.js",
    "build": "gobble --env=build --force build dist"
  }
}
